    "eslint-config-next": "13.3.0",
    "formik": "^2.2.9",
    "next": "13.3.0",
    "postcss": "8.4.23",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-player": "^2.12.0",
